{
  "private": true,
  "name": "react-beautify",
  "displayName": "react-beautify",
  "description": "Beautify JSX Code",
  "version": "0.1.0",
  "publisher": "taichi",
  "galleryBanner": {
    "color": "#61dafb",
    "theme": "light"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other",
    "Languages"
  ],
  "license": "MIT",
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onCommand:react.beautify"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": {
      "title": "react-beautify configuration",
      "properties": {
        "react.beautify.onSave": {
          "description": "Automatically format files on save.",
          "type": "boolean",
          "default": false
        },
        "react.beautify.formatter": {
          "description": "Formatter implementation",
          "type": "string",
          "enum": [
            "prettydiff",
            "esformatter"
          ],
          "default": "prettydiff"
        },
        "react.beautify.configFilePath": {
          "description": "Specifies the workspace relative config filepath",
          "type": "string",
          "default": ".jsbeautifyrc"
        }
      }
    },
    "commands": [
      {
        "command": "react.beautify",
        "title": "Beautify JS/JSX"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "concurrently \"node ./node_modules/vscode/bin/install\" \"typings install\"",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "esformatter": "^0.9.6",
    "esformatter-jsx": "^7.0.1",
    "lodash": "^4.16.0",
    "prettydiff": "^1.16.37",
    "resolve-from": "^2.0.0",
    "strip-json-comments": "^2.0.1"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taichi/react-beautify"
  },
  "bugs": {
    "url": "https://github.com/taichi/react-beautify/issues"
  }
}
